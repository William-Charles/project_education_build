<div class="col-sm-4 col-sm-offset-4">
</div>
<div class="well col-sm-8 col-sm-offset-2">
  <h2>New Course</h2>
  <form action="/teacher/addCourse" method="post">
    <div class="form-group">
      <label for="interest">Interest</label>
      <input type="text" name="interest" value="" class="form-control" required="">
    </div>
    <div class="form-group">
      <label for="category">Category</label>
      <input type="text" name="category" value="" class="form-control" required="">
    </div>
    <div class="form-group">
      <label for="name">Title</label>
      <input type="text" name="name" value="" class="form-control" required="">
    </div>
    <div class="form-group">
      <label for="desc">Description</label>
      <textarea type="text" name="desc" value="" class="form-control" required=""></textarea>
    </div>
    <div class="form-group">
      <label for="outline">Outline</label>
      <textarea type="email" name="outline" value="" class="form-control" required=""></textarea>
    </div>
    <div class="form-group">
      <label for="image">Course Image</label>
      <input type="text" name="image" minlength="8" maxlength="22" class="form-control" required="">
    </div>
    <button class="btn btn-success">Add Course</button>
  </form>
</div>
